{
    "env": {
        "es2021": true
    },
    "global": {
    "__DEV__":"readonly"
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "typescript-eslint",
        "react-hooks"
    ],
    "rules": {
        "prettier/prattier": ["error",{
            "endOfLine": "auto"}
        ],
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhausted-deps": "warn",
        "react/jsx-filename-extension" : [1, {"extensions": [".tsx"] }],
        "import/prefer-default-export": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "ts": "never",
                "tsx": "naver"
            }
        ]
    },
    "settings": {
        "import/resolver":{
            "typescript":{}
        }
    }
}
